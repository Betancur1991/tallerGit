Proceso sin_titulo
	
	definir NombreMascota Como Caracter;
	Escribir "Cual es el nombre de la mascota?";
	Leer NombreMascota;
	
	definir TipoDeMascota Como Caracter;
	Escribir "Cual es el tipo de la mascota?";
	Leer TipoDeMascota;
	
	definir EdadMascota Como Caracter;
	Escribir "Cual es la edad de la mascota?";
	Leer EdadMascota;
	
	definir NombrePropietario Como Caracter;
	Escribir "Cual es el nombre del dueño de la mascota?";
	Leer NombrePropietario;
	
	escribir NombreMascota, " es un ", TipoDeMascota, " el cual, tiene ", EdadMascota, " años de edad y ", NombrePropietario, " es actualmente su dueño.";




	
FinProceso
